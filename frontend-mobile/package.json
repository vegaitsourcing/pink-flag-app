{
  "name": "PinkFlagApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:android": "rimraf android/app/build",
    "run:android-build-dev": "react-native run-android --no-packager",
    "android": "npm-run-all -s clean:android run:android-build-dev start",
    "ios": "react-native run-ios",
    "start": "rimraf ./node_modules/.cache/babel-loader && react-native start --reset-cache",
    "build": "yarn; cd ios; bundle install; bundle exec pod install; cd ..",
    "test": "jest",
    "dependency:check": "madge --circular --extensions ts,js,tsx ./",
    "type:check": "tsc",
    "code:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "code:format": "lint-staged",
    "prepare": "cd .. && husky install frontend-mobile/.husky",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@emotion/native": "^11.10.0",
    "@emotion/react": "^11.10.4",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-picker/picker": "^2.4.8",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@react-navigation/stack": "^6.3.2",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/react-redux": "^7.1.24",
    "axios": "0.27.2",
    "patch-package": "^6.5.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-calendars": "^1.1293.0",
    "react-native-config": "^1.4.11",
    "react-native-date-picker": "^4.2.6",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-pager-view": "^6.0.2",
    "react-native-pagination": "^1.5.5",
    "react-native-reanimated": "^2.13.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.4.0",
    "react-native-wheel-scrollview-picker": "^2.0.1",
    "react-redux": "^8.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "17.0.43",
    "@types/react-native": "^0.70.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "madge": "^5.0.1",
    "metro-react-native-babel-preset": "0.72.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "resolutions": {
    "@types/react": "17.0.2"
  }
}
